[user]
    name = Anton Rogov
    email = anton@blackbits.pro
[alias]
    aa = add --all
    co = checkout
    amend = commit --amend
    pick = cherry-pick
    changes = whatchanged -p --abbrev-commit --pretty=medium
[color]
    branch = auto
    status = auto
    diff = auto
    log = auto
    grep = auto
[core]
    editor = vim
    excludesfile = ~/.gitignore
[apply]
    whitespace = nowarn
[heroku]
    account = blackbits
